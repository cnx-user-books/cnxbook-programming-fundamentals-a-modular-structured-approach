    <section id="id17330715">
      <title>Exercises</title>
<exercise id="fs-id10833574">
  <problem id="fs-id12822683">
    <list id="fs-id10844484" list-type="enumerated">
      <title> Evaluate the following items involving unary positive and unary negative:</title>
        <item>+13 - -2</item>
        <item>-10 + 14</item>
        <item>4 + - 3</item>
        <item>+8 - * +5</item>
  </list>
  </problem>
  <solution id="fs-id10912690">
    <list id="fs-id10826960" list-type="enumerated"><title>Answers:</title><item>15</item>
	<item>4</item>
	<item>1</item>
	<item>Error, no operand between subtraction and multiplication.</item>
</list>
  </solution>
</exercise>
